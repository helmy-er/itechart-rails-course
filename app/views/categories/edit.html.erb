<h1>Editing Category</h1>

<% cat_id=params.require(:format) %>
<%= form_with scope: :category, url: category_path(cat_id),method: :PATCH do |form| %>
  <%= form.label :имя %>
  <%= form.text_field :name %>
  <p>
    Создать для всех
    <%=form.check_box :for_all %>
    <br>
    статус:доход?
    <%=form.check_box :status %>
  </p>
  <%= form.submit %>
<% end %>

<%= link_to 'Back', categories_path(@person_id),class:"one" %>
