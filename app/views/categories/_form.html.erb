<% cat_id=params.require(:people_id) %>
<%= form_with scope: :category, url: category_path(cat_id) do |form| %>
  <%= form.label :имя %>
  <%= form.text_field :name %>
  <p>
    <%= form.label :'создать для всех' %>
  <%=form.check_box :for_all %>
    <%= form.label :'доход' %>
    <%=form.check_box :status %>
  </p>
    <%= form.submit %>
<% end %>